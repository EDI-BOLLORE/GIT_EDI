<edimap xmlns:medi="http://www.milyn.org/schema/edi-message-mapping-1.4.xsd">
    <import namespace="UN" resource="__modelset_definitions.xml"/>
    <description name="LRECLM" version="D:07A:UN"/>
    <delimiters component=":" escape="?" field="+" segment="'!$" sub-component="~"/>
    <segments xmltag="LRECLM">
        <segment ignoreUnmappedFields="false" maxOccurs="1" minOccurs="1" nodeTypeRef="UN:BGM" segcode="BGM" truncatable="true" xmltag="Beginning_of_message">
            <documentation>BGM,
       A segment specifying the type of message contained in the
       transmission.</documentation>
        </segment>
        <segment ignoreUnmappedFields="false" maxOccurs="1" minOccurs="1" nodeTypeRef="UN:DTM" segcode="DTM" truncatable="true" xmltag="Date_time_period">
            <documentation>DTM,
       A segment specifying the reporting period ending date.</documentation>
        </segment>
        <segment ignoreUnmappedFields="false" maxOccurs="1" minOccurs="0" nodeTypeRef="UN:CUX" segcode="CUX" truncatable="true" xmltag="Currencies">
            <documentation>CUX,
       A segment identifying the default currency and its scaling
       factor for monetary amounts in this message.</documentation>
        </segment>
        <segment ignoreUnmappedFields="false" maxOccurs="9" minOccurs="0" nodeTypeRef="UN:CTA" segcode="CTA" truncatable="true" xmltag="Contact_information">
            <documentation>CTA,
       A segment identifying a person(s) or department(s), and their
       function(s), to whom communications should be directed.</documentation>
        </segment>
        <segment ignoreUnmappedFields="false" maxOccurs="1" minOccurs="0" nodeTypeRef="UN:FTX" segcode="FTX" truncatable="true" xmltag="Free_text">
            <documentation>FTX,
       A segment with free text information, in coded or clear form,
       used when additional information is needed but cannot be
       accommodated within other segments.
4.1.2  Detail section
       Information to be provided in the Detail section:</documentation>
        </segment>
        <segmentGroup maxOccurs="999999" minOccurs="1" xmltag="Segment_group_1">
            <documentation>Segment group 1:
       A group of segments in which is detailed claim information.</documentation>
            <segment ignoreUnmappedFields="false" maxOccurs="1" minOccurs="1" nodeTypeRef="UN:IMD" segcode="IMD" truncatable="true" xmltag="Item_description">
                <documentation>IMD,
          A segment defining the type of claim.</documentation>
            </segment>
            <segment ignoreUnmappedFields="false" maxOccurs="99" minOccurs="1" nodeTypeRef="UN:ATT" segcode="ATT" truncatable="true" xmltag="Attribute">
                <documentation>ATT,
          A segment providing attributes of the claim.</documentation>
            </segment>
            <segment ignoreUnmappedFields="false" maxOccurs="9" minOccurs="0" nodeTypeRef="UN:DTM" segcode="DTM" truncatable="true" xmltag="Date_time_period">
                <documentation>DTM,
          A segment specifying general dates or durations related to
          the claim.</documentation>
            </segment>
            <segment ignoreUnmappedFields="false" maxOccurs="1" minOccurs="0" nodeTypeRef="UN:LOC" segcode="LOC" truncatable="true" xmltag="Place_location_identification">
                <documentation>LOC,
          A segment giving location information for the place of death
          or location where the onset of the disability occurred.</documentation>
            </segment>
            <segment ignoreUnmappedFields="false" maxOccurs="9" minOccurs="0" nodeTypeRef="UN:PCD" segcode="PCD" truncatable="true" xmltag="Percentage_details">
                <documentation>PCD,
          A segment specifying the extent of loss and the interest
          rate to be used in calculating the claim payment.</documentation>
            </segment>
            <segment ignoreUnmappedFields="false" maxOccurs="9" minOccurs="0" nodeTypeRef="UN:RFF" segcode="RFF" truncatable="true" xmltag="Reference">
                <documentation>RFF,
          A segment specifying the claim numbers as assigned by the
          ceding company and the reinsurer.</documentation>
            </segment>
            <segment ignoreUnmappedFields="false" maxOccurs="1" minOccurs="0" nodeTypeRef="UN:CUX" segcode="CUX" truncatable="true" xmltag="Currencies">
                <documentation>CUX,
          A segment identifying the default currency and its scaling
          factor for monetary amounts for the claim.</documentation>
            </segment>
            <segment ignoreUnmappedFields="false" maxOccurs="1" minOccurs="0" nodeTypeRef="UN:FTX" segcode="FTX" truncatable="true" xmltag="Free_text">
                <documentation>FTX,
          A segment with free text information, in coded or clear
          form, used when additional information is needed regarding
          the claim.</documentation>
            </segment>
            <segmentGroup maxOccurs="9" minOccurs="0" xmltag="Segment_group_2">
                <documentation>Segment group 2:
          A group of segments in which is detailed monetary amounts
          and their purpose for the claim.</documentation>
                <segment ignoreUnmappedFields="false" maxOccurs="1" minOccurs="1" nodeTypeRef="UN:MOA" segcode="MOA" truncatable="true" xmltag="Monetary_amount">
                    <documentation>MOA,
             A segment specifying a monetary amount.</documentation>
                </segment>
                <segment ignoreUnmappedFields="false" maxOccurs="1" minOccurs="1" nodeTypeRef="UN:ARD" segcode="ARD" truncatable="true" xmltag="Monetary_amount_function">
                    <documentation>ARD,
             A segment specifying the purpose of the monetary amount.</documentation>
                </segment>
            </segmentGroup>
            <segmentGroup maxOccurs="1" minOccurs="1" xmltag="Segment_group_3">
                <documentation>Segment group 3:
          A group of segments in which is detailed information
          regarding the person for which this claim is being
          submitted.</documentation>
                <segment ignoreUnmappedFields="false" maxOccurs="1" minOccurs="1" nodeTypeRef="UN:PNA" segcode="PNA" truncatable="true" xmltag="Party_identification">
                    <documentation>PNA,
             A segment identifying the name of the person on claim.</documentation>
                </segment>
                <segment ignoreUnmappedFields="false" maxOccurs="9" minOccurs="1" nodeTypeRef="UN:DTM" segcode="DTM" truncatable="true" xmltag="Date_time_period">
                    <documentation>DTM,
             A segment specifying general date and durations
             associated with the person on claim.</documentation>
                </segment>
                <segment ignoreUnmappedFields="false" maxOccurs="1" minOccurs="0" nodeTypeRef="UN:LOC" segcode="LOC" truncatable="true" xmltag="Place_location_identification">
                    <documentation>LOC,
             A segment giving location information for the place of
             residence of the person on claim at the time of loss.</documentation>
                </segment>
                <segment ignoreUnmappedFields="false" maxOccurs="1" minOccurs="0" nodeTypeRef="UN:EMP" segcode="EMP" truncatable="true" xmltag="Employment_details">
                    <documentation>EMP,
             A segment providing the occupation class of the person on
             claim at time of loss for purposes of this claim as
             defined by the issuing company.</documentation>
                </segment>
                <segmentGroup maxOccurs="9" minOccurs="0" xmltag="Segment_group_4">
                    <documentation>Segment group 4:
             A group of segments in which is detailed monetary amounts
             and their purpose for this person on claim.</documentation>
                    <segment ignoreUnmappedFields="false" maxOccurs="1" minOccurs="1" nodeTypeRef="UN:MOA" segcode="MOA" truncatable="true" xmltag="Monetary_amount">
                        <documentation>MOA,
                A segment specifying a monetary amount.</documentation>
                    </segment>
                    <segment ignoreUnmappedFields="false" maxOccurs="1" minOccurs="1" nodeTypeRef="UN:ARD" segcode="ARD" truncatable="true" xmltag="Monetary_amount_function">
                        <documentation>ARD,
                A segment specifying the purpose of the monetary
                amount.</documentation>
                    </segment>
                </segmentGroup>
            </segmentGroup>
            <segmentGroup maxOccurs="99" minOccurs="1" xmltag="Segment_group_5">
                <documentation>Segment group 5:
          A group of segments in which is detailed information
          regarding the policy on claim and other policies related to
          the claim.</documentation>
                <segment ignoreUnmappedFields="false" maxOccurs="1" minOccurs="1" nodeTypeRef="UN:GEI" segcode="GEI" truncatable="true" xmltag="Processing_information">
                    <documentation>GEI,
             An indicator specifying whether this policy is the policy
             on claim or an associated policy.</documentation>
                </segment>
                <segment ignoreUnmappedFields="false" maxOccurs="9" minOccurs="1" nodeTypeRef="UN:RFF" segcode="RFF" truncatable="true" xmltag="Reference">
                    <documentation>RFF,
             A segment specifying the policy number as assigned by the
             ceding company.</documentation>
                </segment>
                <segment ignoreUnmappedFields="false" maxOccurs="1" minOccurs="0" nodeTypeRef="UN:PNA" segcode="PNA" truncatable="true" xmltag="Party_identification">
                    <documentation>PNA,
             A segment identifying the original issuer of the policy,
             in coded or clear form.</documentation>
                </segment>
                <segment ignoreUnmappedFields="false" maxOccurs="99" minOccurs="0" nodeTypeRef="UN:DTM" segcode="DTM" truncatable="true" xmltag="Date_time_period">
                    <documentation>DTM,
             A segment specifying general dates and durations
             associated with the policy on claim or related policy.</documentation>
                </segment>
                <segmentGroup maxOccurs="9" minOccurs="0" xmltag="Segment_group_6">
                    <documentation>Segment group 6:
             A group of segments in which is detailed monetary amounts
             and their purpose for this policy.</documentation>
                    <segment ignoreUnmappedFields="false" maxOccurs="1" minOccurs="1" nodeTypeRef="UN:MOA" segcode="MOA" truncatable="true" xmltag="Monetary_amount">
                        <documentation>MOA,
                A segment specifying a monetary amount.</documentation>
                    </segment>
                    <segment ignoreUnmappedFields="false" maxOccurs="1" minOccurs="1" nodeTypeRef="UN:ARD" segcode="ARD" truncatable="true" xmltag="Monetary_amount_function">
                        <documentation>ARD,
                A segment specifying the purpose of the monetary
                amount.</documentation>
                    </segment>
                </segmentGroup>
                <segmentGroup maxOccurs="99" minOccurs="0" xmltag="Segment_group_7">
                    <documentation>Segment group 7:
             A group of segments in which is detailed information
             regarding the insureds on this policy.</documentation>
                    <segment ignoreUnmappedFields="false" maxOccurs="1" minOccurs="1" nodeTypeRef="UN:IDE" segcode="IDE" truncatable="true" xmltag="Identity">
                        <documentation>IDE,
                A segment identifying the party as the insured of
                record.</documentation>
                    </segment>
                    <segment ignoreUnmappedFields="false" maxOccurs="1" minOccurs="1" nodeTypeRef="UN:PNA" segcode="PNA" truncatable="true" xmltag="Party_identification">
                        <documentation>PNA,
                A segment identifying the insured's name and universal
                identifier.</documentation>
                    </segment>
                    <segment ignoreUnmappedFields="false" maxOccurs="1" minOccurs="1" nodeTypeRef="UN:DTM" segcode="DTM" truncatable="true" xmltag="Date_time_period">
                        <documentation>DTM,
                A segment specifying the date of birth of the insured.</documentation>
                    </segment>
                    <segmentGroup maxOccurs="99" minOccurs="0" xmltag="Segment_group_8">
                        <documentation>Segment group 8:
                A group of segments in which is detailed coverage
                information regarding the insured.</documentation>
                        <segment ignoreUnmappedFields="false" maxOccurs="1" minOccurs="1" nodeTypeRef="UN:ICD" segcode="ICD" truncatable="true" xmltag="Insurance_cover_description">
                            <documentation>ICD,
                   A segment giving the insurance cover type
                   associated with this coverage.</documentation>
                        </segment>
                        <segment ignoreUnmappedFields="false" maxOccurs="99" minOccurs="0" nodeTypeRef="UN:DTM" segcode="DTM" truncatable="true" xmltag="Date_time_period">
                            <documentation>DTM,
                   A segment specifying general dates or durations
                   related to this coverage.</documentation>
                        </segment>
                        <segment ignoreUnmappedFields="false" maxOccurs="9" minOccurs="0" nodeTypeRef="UN:RFF" segcode="RFF" truncatable="true" xmltag="Reference">
                            <documentation>RFF,
                   A segment specifying general references for this
                   coverage.</documentation>
                        </segment>
                        <segment ignoreUnmappedFields="false" maxOccurs="9" minOccurs="0" nodeTypeRef="UN:ATT" segcode="ATT" truncatable="true" xmltag="Attribute">
                            <documentation>ATT,
                   A segment providing attributes of the coverage.</documentation>
                        </segment>
                        <segment ignoreUnmappedFields="false" maxOccurs="1" minOccurs="0" nodeTypeRef="UN:PCD" segcode="PCD" truncatable="true" xmltag="Percentage_details">
                            <documentation>PCD,
                   A segment specifying the percentages associated
                   with the claim.</documentation>
                        </segment>
                        <segment ignoreUnmappedFields="false" maxOccurs="1" minOccurs="0" nodeTypeRef="UN:AGR" segcode="AGR" truncatable="true" xmltag="Agreement_identification">
                            <documentation>AGR,
                   A segment identifying the treaty as assigned by the
                   ceding company.</documentation>
                        </segment>
                        <segmentGroup maxOccurs="9" minOccurs="0" xmltag="Segment_group_9">
                            <documentation>Segment group 9:
                   A group of segments in which is detailed monetary
                   amounts and their purpose for the coverage.</documentation>
                            <segment ignoreUnmappedFields="false" maxOccurs="1" minOccurs="1" nodeTypeRef="UN:MOA" segcode="MOA" truncatable="true" xmltag="Monetary_amount">
                                <documentation>MOA,
                      A segment specifying a monetary amount.</documentation>
                            </segment>
                            <segment ignoreUnmappedFields="false" maxOccurs="1" minOccurs="1" nodeTypeRef="UN:ARD" segcode="ARD" truncatable="true" xmltag="Monetary_amount_function">
                                <documentation>ARD,
                      A segment specifying the purpose of the monetary
                      amount.</documentation>
                            </segment>
                        </segmentGroup>
                    </segmentGroup>
                </segmentGroup>
            </segmentGroup>
        </segmentGroup>
    </segments>
</edimap>

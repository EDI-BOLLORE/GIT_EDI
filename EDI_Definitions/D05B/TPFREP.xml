<edimap xmlns:medi="http://www.milyn.org/schema/edi-message-mapping-1.4.xsd">
    <import namespace="UN" resource="__modelset_definitions.xml"/>
    <description name="TPFREP" version="D:05B:UN"/>
    <delimiters component=":" escape="?" field="+" segment="'!$" sub-component="~"/>
    <segments xmltag="TPFREP">
        <segment ignoreUnmappedFields="false" maxOccurs="1" minOccurs="1" nodeTypeRef="UN:BGM" segcode="BGM" truncatable="true" xmltag="Beginning_of_message">
            <documentation>BGM,
       A segment to indicate the beginning of the message by
       identifying the message by a message number.</documentation>
        </segment>
        <segment ignoreUnmappedFields="false" maxOccurs="1" minOccurs="0" nodeTypeRef="UN:DTM" segcode="DTM" truncatable="true" xmltag="Date_time_period">
            <documentation>DTM,
       A segment to identify a date and time on which the message is
       sent.</documentation>
        </segment>
        <segmentGroup maxOccurs="1" minOccurs="1" xmltag="Segment_group_1">
            <documentation>Segment group 1:
       A group of segments to identify the means of transport handled
       by the terminal operator.</documentation>
            <segment ignoreUnmappedFields="false" maxOccurs="1" minOccurs="1" nodeTypeRef="UN:TDT" segcode="TDT" truncatable="true" xmltag="Transport_information">
                <documentation>TDT,
          A segment to identify the means of transport.</documentation>
            </segment>
            <segment ignoreUnmappedFields="false" maxOccurs="1" minOccurs="1" nodeTypeRef="UN:LOC" segcode="LOC" truncatable="true" xmltag="Place_location_identification">
                <documentation>LOC,
          A segment to identify the location where the means of
          transport is handled.</documentation>
            </segment>
            <segment ignoreUnmappedFields="false" maxOccurs="9" minOccurs="0" nodeTypeRef="UN:DTM" segcode="DTM" truncatable="true" xmltag="Date_time_period">
                <documentation>DTM,
          A segment to identify dates and times on which the means of
          transport is handled.</documentation>
            </segment>
            <segment ignoreUnmappedFields="false" maxOccurs="9" minOccurs="0" nodeTypeRef="UN:RFF" segcode="RFF" truncatable="true" xmltag="Reference">
                <documentation>RFF,
          A segment to identify relevant reference numbers for the
          means of transport and the operations.</documentation>
            </segment>
        </segmentGroup>
        <segmentGroup maxOccurs="99" minOccurs="0" xmltag="Segment_group_2">
            <documentation>Segment group 2:
       A group of segments to specify the details of the equipment and
       the operations performed.</documentation>
            <segment ignoreUnmappedFields="false" maxOccurs="1" minOccurs="1" nodeTypeRef="UN:EQD" segcode="EQD" truncatable="true" xmltag="Equipment_details">
                <documentation>EQD,
          A segment to specify details of the equipment, such as the
          equipment identification number.</documentation>
            </segment>
            <segment ignoreUnmappedFields="false" maxOccurs="99" minOccurs="0" nodeTypeRef="UN:CNT" segcode="CNT" truncatable="true" xmltag="Control_total">
                <documentation>CNT,
          A segment to specify control totals.</documentation>
            </segment>
            <segmentGroup maxOccurs="999" minOccurs="0" xmltag="Segment_group_3">
                <documentation>Segment group 3:
          A group of segments to specify dates and times and other
          details for the operations carried out.</documentation>
                <segment ignoreUnmappedFields="false" maxOccurs="1" minOccurs="1" nodeTypeRef="UN:DTM" segcode="DTM" truncatable="true" xmltag="Date_time_period">
                    <documentation>DTM,
             A segment to identify a date and time related to the
             equipment operation.</documentation>
                </segment>
                <segment ignoreUnmappedFields="false" maxOccurs="9" minOccurs="0" nodeTypeRef="UN:FTX" segcode="FTX" truncatable="true" xmltag="Free_text">
                    <documentation>FTX,
             A segment to provide detailed information related to the
             equipment operation.</documentation>
                </segment>
            </segmentGroup>
        </segmentGroup>
    </segments>
</edimap>
